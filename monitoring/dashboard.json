{
  "dashboard": {
    "id": null,
    "title": "Schema Registry Overview",
    "tags": ["schema-registry", "nats", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Requests Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Request Duration (95th percentile)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(schema_registry_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "color": {"mode": "thresholds"}
          }
        }
      },
      {
        "id": 3,
        "title": "Schema Registrations",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(schema_registry_registrations_total[5m])",
            "legendFormat": "{{subject}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 4,
        "title": "Schema Retrievals",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "targets": [
          {
            "expr": "rate(schema_registry_retrievals_total[5m])",
            "legendFormat": "{{subject}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 5,
        "title": "Schema Validations",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "targets": [
          {
            "expr": "rate(schema_registry_validations_total[5m])",
            "legendFormat": "{{subject}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "NATS Connections",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "nats_embedded_connections_total",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "color": {"mode": "thresholds"}
          }
        }
      },
      {
        "id": 8,
        "title": "JetStream Memory Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "nats_jetstream_storage_bytes{type=\"memory\"}",
            "legendFormat": "Memory",
            "refId": "A"
          },
          {
            "expr": "nats_jetstream_storage_bytes{type=\"storage\"}",
            "legendFormat": "Storage",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 9,
        "title": "KV Operations",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(nats_kv_operations_total{operation=\"put\"}[5m])",
            "legendFormat": "PUT ops/s",
            "refId": "A"
          },
          {
            "expr": "rate(nats_kv_operations_total{operation=\"get\"}[5m])",
            "legendFormat": "GET ops/s",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 10,
        "title": "Service Health",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"schema-registry-app\"}",
            "legendFormat": "Schema Registry",
            "refId": "A"
          },
          {
            "expr": "up{job=\"nats-embedded\"}",
            "legendFormat": "NATS Embedded",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {"text": "DOWN"},
                  "1": {"text": "UP"}
                }
              }
            ]
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "subject",
          "type": "query",
          "query": "label_values(schema_registry_registrations_total, subject)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "instance",
          "type": "query", 
          "query": "label_values(up, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": false
        }
      ]
    },
    "refresh": "5s",
    "schemaVersion": 36,
    "version": 1
  },
  "folderTitle": "Schema Registry",
  "message": "Schema Registry Dashboard imported",
  "overwrite": true
}